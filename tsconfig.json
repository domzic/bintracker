{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "es2020"
    ],
    "allowJs": true,
    "outDir": "build",
    "baseUrl": "src",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true
  },
  "include": [
    "server/*"
  ]
}
